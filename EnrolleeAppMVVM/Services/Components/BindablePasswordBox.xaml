<UserControl x:Class="EnrolleeAppMVVM.Services.Components.BindablePasswordBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EnrolleeAppMVVM.Services.Components"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="100">
    <Grid>
        <PasswordBox x:Name="passwordBox" Style="{StaticResource MaterialDesignPasswordBox}" PasswordChanged="passwordBox_PasswordChanged">
        </PasswordBox>
    </Grid>
</UserControl>
